{
  "name": "@business-as-code/cli",
  "version": "0.0.1",
  "description": "Tool package @business-as-code/cli",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "publishConfig": {
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "bin": "dist/bin/run.js"
  },
  "keywords": [
    "typescript",
    "generator",
    "oclif"
  ],
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "bin"
  ],
  "bin": {
    "bac": "src/bin/dev.ts"
  },
  "dependencies": {
    "@business-as-code/plugin-core-essentials": "workspace:*",
    "@business-as-code/plugin-core-scaffold": "workspace:*",
    "@business-as-code/plugin-core-git": "workspace:*",
    "@oclif/command": "^1.8.22",
    "@oclif/config": "^1.18.8",
    "@oclif/core": "2.6.4",
    "@oclif/plugin-help": "^5.2.7",
    "@oclif/plugin-not-found": "^2.3.21",
    "@oclif/plugin-plugins": "^2.3.2"
  },
  "devDependencies": {
    "@business-as-code/plugin-core-tests": "workspace:*",
    "@types/node": "18.14.2",
    "typescript": "5.0.2"
  },
  "oclif": {
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    },
    "plugins": [
      "@oclif/plugin-help",
      "@business-as-code/plugin-core-essentials",
      "@business-as-code/plugin-core-git",
      "@business-as-code/plugin-core-scaffold"
    ],
    "devPlugins": [
      "@business-as-code/plugin-core-tests",
      "@oclif/plugin-plugins"
    ]
  }
}
