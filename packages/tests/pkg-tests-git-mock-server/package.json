{
  "name": "@business-as-code/tests-git-server",
  "private": true,
  "version": "0.0.0-latest-20231029192408",
  "description": "Clone and push to git repository test fixtures over HTTP.",
  "main": "index.js",
  "exports": "./index.js",
  "bin": {
    "git-http-mock-server": "./http-daemon.js",
    "git-ssh-mock-server": "./ssh-daemon.js"
  },
  "scripts": {
    "//gitServerHttp:isRunning": "pgrep -f 'http-daemon.js'",
    "///gitServerHttp:isRunning": "node ./http-daemon.js isRunning || lsof -i 4tcp:8174 -sTCP:LISTEN",
    "gitServerHttp:isRunning": "lsof -i 4tcp:8174 -sTCP:LISTEN",
    "gitServerHttp:startBackground": "node ./http-daemon.js start",
    "//gitServerHttp:stopBackground": "node ./http-daemon.js stop || kill-port 8174 || true; ! pnpm run gitServerHttp:isRunning",
    "gitServerHttp:stopBackground": "kill-port 8174 || true",
    "//gitServerSshPubKey:isRunning": "node ./ssh-daemon.js isRunning || lsof -i 4tcp:2222 -sTCP:LISTEN",
    "///gitServerSshPubKey:isRunning": "node ./ssh-daemon.js isRunning",
    "gitServerSshPubKey:isRunning": "lsof -i 4tcp:2222 -sTCP:LISTEN",
    "gitServerSshPubKey:startBackground": "GIT_SSH_MOCK_SERVER_PUBKEY=true node ./ssh-daemon.js start",
    "//gitServerSshPubKey:stopBackground": "GIT_SSH_MOCK_SERVER_PUBKEY=true node ./ssh-daemon.js stop || kill-port 2222 || true; ! pnpm run gitServerSshPubKey:isRunning",
    "gitServerSshPubKey:stopBackground": "kill-port 2222 || true",
    "//gitServerSshPassword:isRunning": "node ./ssh-daemon-password.js isRunning || lsof -i 4tcp:2223 -sTCP:LISTEN",
    "///gitServerSshPassword:isRunning": "node ./ssh-daemon-password.js isRunning",
    "gitServerSshPassword:isRunning": "lsof -i 4tcp:2223 -sTCP:LISTEN",
    "gitServerSshPassword:startBackground": "GIT_SSH_MOCK_SERVER_PASSWORD=password node ./ssh-daemon-password.js start",
    "//gitServerSshPassword:stopBackground": "GIT_SSH_MOCK_SERVER_PASSWORD=password node ./ssh-daemon-password.js stop || kill-port 2223 || true; ! pnpm run gitServerSshPassword:isRunning",
    "gitServerSshPassword:stopBackground": "kill-port 2223 || true",
    "//gitServerSshAnonymous:isRunning": "node ./ssh-daemon-anonymous.js isRunning || lsof -i 4tcp:2224 -sTCP:LISTEN",
    "///gitServerSshAnonymous:isRunning": "node ./ssh-daemon-anonymous.js isRunning",
    "gitServerSshAnonymous:isRunning": "lsof -i 4tcp:2224 -sTCP:LISTEN",
    "gitServerSshAnonymous:startBackground": "node ./ssh-daemon-anonymous.js start",
    "//gitServerSshAnonymous:stopBackground": "node ./ssh-daemon-anonymous.js stop || kill-port 2224 || true; ! pnpm run gitServerSshAnonymous:isRunning",
    "gitServerSshAnonymous:stopBackground": "kill-port 2224 || true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/isomorphic-git/git-http-mock-server.git"
  },
  "keywords": [
    "karma-plugin",
    "karma-middleware",
    "git-http-server",
    "git-http-backend"
  ],
  "author": "William Hilton <wmhilton@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/isomorphic-git/git-http-mock-server/issues"
  },
  "homepage": "https://github.com/isomorphic-git/git-http-mock-server#readme",
  "dependencies": {
    "@business-as-code/core": "workspace:*",
    "basic-auth": "^2.0.1",
    "buffer-equal-constant-time": "^1.0.1",
    "chalk": "^2.4.2",
    "daemonize-process": "^1.0.9",
    "fixturez": "^1.1.0",
    "htpasswd-js": "^1.0.2",
    "micro-cors": "^0.1.1",
    "minimisted": "^2.0.1",
    "ssh-keygen": "^0.4.2",
    "ssh2": "file:///Users/matt/dev/tmp/bac-tests/local-npm-packages/ssh2/ssh2-1.14.0.tgz",
    "tree-kill": "^1.2.2"
  },
  "devDependencies": {
    "kill-port": "1.6.1"
  },
  "trustedDependencies": [
    "ssh2"
  ]
}